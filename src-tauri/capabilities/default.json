{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Default capability for the main window",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "core:app:default",
        "core:event:default",
        "core:event:allow-listen",
        "core:event:allow-emit",
        "core:window:default",
        "core:webview:default",
        "shell:default",
        "opener:default",
        "dialog:default",
        "dialog:allow-open"
    ]
}